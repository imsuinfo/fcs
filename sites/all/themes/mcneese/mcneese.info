name = McNeese Theme (4.0)
description = "This is the baseline McNeese theme. It is HTML5 based and supports multipe alternate themes.
version = "7.x-4.0"
core = 7.x
engine = phptemplate
screenshot = images/screenshot.png

; project is not hosted on drupal.org, so manual versioning is needed.
version = "7.x-4.0"

dependencies[] = cf
dependencies[] = cf_theme


; the cache files, starting with c, such as css/all/c000-all.css, are generated by concating all of the files in the same directory in their alphanumeric order.
stylesheets[all][] = css/classic/all/c000-all.css
stylesheets[screen][] = css/classic/screen/c001-screen.css
stylesheets[speech][] = css/classic/braille/c002-braille.css
stylesheets[print][] = css/classic/print/c003-print.css

scripts[] = js/c000-js.js


regions[top]           = Top
regions[header]        = Header
regions[header_menu_1] = Header Menu 1
regions[header_menu_2] = Header Menu 2

regions[bulletin]    = Bulletin
regions[messages]    = Messages
regions[help]        = Help
regions[information] = Information

regions[menus]   = Menus
regions[asides]  = Asides
regions[content] = Content
regions[footer]  = Footer
regions[bottom]  = Bottom
regions[hidden]  = Hidden


features[] = name
features[] = search
features[] = favicon


settings[shortcut_module_link] = 1
